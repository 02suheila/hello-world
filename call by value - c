#include <stdio.h>
void swap (int a,int b);
int main()
{
    int a=10,b=20;
    printf("before function call a=%d b=%d",a,b);
    swap(a,b);
     printf("after function call a=%d b=%d",a,b);
     return 0;
}
void swap(int a,int b)
{
    int tem;
    tem =a;
    a=b;
    b=tem;
     printf("\n \n function call a=%d b=%d",a,b);
}
   
