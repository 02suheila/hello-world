#include <stdio.h>

int main() {
    int temp,sum=0,num,rem;
    printf("enter a number");
    scanf("%d",&num);
    temp=num;
    while(num!=0)
    {
        rem=num%10;
        num=num/10;
        sum=sum*10+rem;
    }
    if(temp==sum)
    {
        printf("palindrome number");
    }
    else{
        printf("not a palindrome number");
    }
    return 0;
}
