#include<stdio.h>
int main()
{
    int a[10][10],b[10][10],c[10][10],i,j,k;
    printf("enter elements into A matrix");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
{
    scanf("%d",&a[i][j]);
}
    }
        printf("enter elements into b matrix");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
{
    c[i][j]=0;
    for(k=0;k<3;k++)
    {
        c[i][j]=c[i][j]+a[i][k]*b[k][j];
    }
}
} for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
{
    printf("%d",c[i][j]);
}
printf("\n");
}
return 0;
}





