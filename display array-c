#include <stdio.h>

int main()
{
    int x[50];
    int i,n;
    printf("enter number of array elements:");
    scanf("%d",&n);
    printf("enter array elements");
    for(i=0;i<n;i++)
    {
        scanf("%d",&x[i]);
    }
        printf("\n");
        for(i=0;i<n;i++)
        {
            printf("%d",x[i]);
        }
    
    return 0;
}
